{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) train",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/create",
            "args": [
                "-show",
                "-img",
                "./train/lena.jpg",
                "-vec",
                "./train/train.vec"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) info",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/create",
            "args": [
                "-show",
                "10",
                "-vec",
                "./train/train.vec",
                "-num",
                "1",
                "-info",
                "./train/info.dat"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) show",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/create",
            "args": [
                "-show",
                "10",
                "-vec",
                "./train/trainingfaces_24-24.vec"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}